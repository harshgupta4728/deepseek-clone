{
  "name": "deepseek-ai-clone",
  "version": "1.0.0",
  "description": "A complete clone of DeepSeek AI with frontend and backend",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup": "node setup.js"
  },
  "keywords": [
    "ai",
    "chat",
    "deepseek",
    "clone"
  ],
  "author": "DeepSeek AI Clone",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react-scripts": "^5.0.1"
  }
}
